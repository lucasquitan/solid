{
    "$schema": "https://json.schemastore.org/tsconfig",
    "_version": "20.1.0",
      "compilerOptions": {
      "lib": ["es2023", "dom"],
      "module": "CommonJS",
      "target": "es2022",
      "baseUrl": "./",
      "paths": {
        "@/*" : ["./src/*"]
      },
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node"
    }
  }